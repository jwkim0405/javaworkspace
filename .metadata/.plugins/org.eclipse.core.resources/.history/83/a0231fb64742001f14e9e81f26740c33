package java02_variable;

public class Var02_TypeCasting {
    public static void main(String[] args) {
        // 형 변환 : 자료형을 바꾸는 것
    	
    	// 1. 묵시적 형 변환
    	// 	- 범위가 작은 자료형 -> 범위가 큰 자료형으로 이사
    	int n1 = 100;
    	float f1 = 100;
    	System.out.printf("%f\n", f1);
    	
    	// 2. 명시적 형 변환
    	//	- 범위가 큰 자료형 -> 범위가 작은 자료형으로 이사
    	
    	
    	
        
        
    }
}
